{
  "name": "@saas-starter-with-monorepo/utils",
  "description": "Shared utility functions.",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "src"
  ],
  "exports": {
    "./sample-function": "./src/sampleFunction.ts"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf .turbo node_modules/.cache vitest-output",
